package sec1;

public class Array3 {

	public static void main(String[] args) {
		
		String[] names = {"이규진", "윤연실", "이규현", "박태우", "김선기"};
		//5명의 3과목 점수 선언 jum[][]
		int[][] jum = {{60,70,80},{100,50,60},{100,100,90},{30,20,40}, {50,60,50}};
		
		//	전체총점(hap), 전체평균(py), 과목총점(tot), 과목평균(avg)
		int hap = 0;
		double py = 0.0;
		int[] tot;
		double[] avg;
		
		System.out.println("성명\t 국어\t 영어\t 수학\t");
		System.out.println(names[0]+"\t" + jum);
		System.out.println(jum[3]);

	}

}
