package sec2;

import java.util.Scanner;

public class Example7 {

	public static void main(String[] args) {
		
		boolean run = true;
		int balance = 0;
		Scanner sc = new Scanner(System.in);
		while(run) {
		System.out.println("--------------------------");
		System.out.println("1.예금 | 2.출금 | 3.잔고 | 4.종료");
		System.out.println("--------------------------");
		break;
		}
		balance = sc.nextInt();
		System.out.println("선택> ");
		System.out.println("예금액");
	}

}
